<table>
 

  <tbody>
    <% @vote_pics.each do |vote_pic| %>
      <br />
      <td>
        <tr><%= image_tag(vote_pic.src,art:vote_pic.src) %></tr>
        <tr><%= pluralize(vote_pic.votes.count, "ä¸ª") %></tr>
        <tr><%= button_to '+1', upvote_vote_pic_path(vote_pic), method: :post %></tr>
        <tr><%= button_to '-1', downvote_vote_pic_path(vote_pic), method: :post %></tr>
        <br />
       </td>
    <% end %>
  </tbody>
</table>


